.svn/
.hg/

# Dependencies (language-agnostic)
node_modules/
vendor/
packages/

# Build outputs
dist/
build/
out/
bin/
obj/
target/

# Logs
*.log

# Environment
.env
.env.local

# OS files
.DS_Store
Thumbs.db
desktop.ini

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# Temporary files
*.tmp
