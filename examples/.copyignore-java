target/
build/
out/
bin/

# Class files
*.class
*.jar
*.war
*.ear

# Gradle
.gradle/
gradle-app.setting
!gradle-wrapper.jar

# Maven
.mvn/
mvnw
mvnw.cmd

# IDE
.idea/
*.iml
*.ipr
*.iws
.vscode/
.classpath
.project
.settings/
*.swp
*.swo
*~

# Eclipse
.metadata/
bin/
tmp/
*.tmp
*.bak
*.swp
*~.nib
local.properties

# NetBeans
nbproject/private/
build/
nbbuild/
dist/
nbdist/
.nb-gradle/

# Logs
*.log

# OS files
.DS_Store
